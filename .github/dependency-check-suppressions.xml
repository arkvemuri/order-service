<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
    Example suppressions for common false positives.
    Remove or modify these based on your specific needs.
    -->
    
    <!-- Suppress false positives for test dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppressing test dependencies that are not part of the runtime
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\..*@.*$</packageUrl>
        <cpe>cpe:/a:junit:junit</cpe>
    </suppress>
    
    <!-- Suppress Spring Boot starter vulnerabilities that are handled by framework updates -->
    <suppress>
        <notes><![CDATA[
        Spring Boot framework handles security updates through version management
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-starter.*@.*$</packageUrl>
        <vulnerabilityName regex="true">.*CVE-2021-22060.*</vulnerabilityName>
    </suppress>
    
    <!-- Example: Suppress specific CVE if it doesn't apply to your usage -->
    <!--
    <suppress>
        <notes><![CDATA[
        This vulnerability doesn't apply to our usage pattern
        ]]></notes>
        <cve>CVE-YYYY-XXXXX</cve>
    </suppress>
    -->
    
    <!-- Example: Suppress by file name pattern -->
    <!--
